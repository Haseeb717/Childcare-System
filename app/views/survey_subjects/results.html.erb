<div id="legacy-result">
    <div class="container">
      <div class="row back-page">
            <div class="col-lg-push-1 col-md-push-1 col-md-9 col-sm-9 col-xs-12 logo-back margin_single">
                <%= render 'subjects' %>
                <div id="bar_graph_partial" class="wrap">
                    <% if @results.count > 0 %>
                        <div class="barGraph">
                            <ul class="graph">
                                <% @results.each_with_index do |result, index| %>
                                    <span class="graph-barBack ">
                                        <li class="graph-bar <%= 'graph-bar2' if index % 2 == 0 %>" data-value="<%= result[1].to_i %>"></li>
                                        <p><%= result[0] %></p>
                                    </span>
                                <% end %>
                            </ul>
                        </div>
                    <% else %>
                        <p>
                            Take the <%= link_to 'test here', new_subject_attempt_path(@subject) %> to see results...
                        </p>
                    <% end %>
                </div>
            </div>
        </div>
    </div>
</div>